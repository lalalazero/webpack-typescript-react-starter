{
    "presets": [
        "@babel/env",
        "@babel/typescript",
        "@babel/react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        [
            "import", {
                "libraryName": "antd",
                "libraryDirectory": "lib",   // default: lib
                "style": true
            }
        ]
    ]
}